if ("<%= escape_javascript( @analyst_target_price ) %>"!="None")
{
	$("#stock_target_price_auto").val("<%= escape_javascript( @analyst_target_price ) %>"); 
	$("#stock_target_price_view").html("<%= escape_javascript( @analyst_target_price ) %>"); 
	$("#targetprice_viz").html("<%= escape_javascript("Analyst target price automatically retrieved from Alphavantage") %>"); 
